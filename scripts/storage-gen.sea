function string intToBase(int num,int base){
    int ab = abs(num)
    if(ab<=base-1)return baseConverterIndex(num,base)
    string str = ""
    int digits = getDigits(num,base)
    int n = 0
    for i=1 until digits{
        int tip = base**i
        int ti = tip/base
        int a = ab-(ab/tip)*tip-n
        str=baseConverterIndex(a/ti,base)+str
        n=n+a
    }
    if(num<0)return "-"+str
    return str
}

function string intToString(int num){return intToBase(num,10)}

function string idify(int num){
    string str = intToBase(num/50,62)
    int n = num/50*50
    int m = num-n+9
    if(m>=10)m=m+1
    if(m>=24)m=m+1
    string str2 = intToBase(m,62)
    if(num==n)return "Z"+str
    if(str!="0")return str2+str
    return str2
}

