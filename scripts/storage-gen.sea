function int abs(int num){
 if(num<0)return num*-1
 return num
}

function string digitToString(int num){
    if(abs(num)>9)return intToString(num)
    string digit = ""
    if(num<0)digit="-"
    int anum = abs(num)
    if(anum==0)digit=digit+"0"
    if(anum==1)digit=digit+"1"
    if(anum==2)digit=digit+"2"
    if(anum==3)digit=digit+"3"
    if(anum==4)digit=digit+"4"
    if(anum==5)digit=digit+"5"
    if(anum==6)digit=digit+"6"
    if(anum==7)digit=digit+"7"
    if(anum==8)digit=digit+"8"
    if(anum==9)digit=digit+"9"
    return digit
}

function int getDigits(int num,int base){
    int digits = 0
    int n = abs(num)
    if(n==0)n=1
    while(n>0){
        n=n/base
        digits=digits+1
    }
    return digits
}

